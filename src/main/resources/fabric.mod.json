{
	"schemaVersion": 1,
	"id": "idwthp",
	"version": "${version}",
	"name": "I Don't Want to Hurt Pets!",
	"description": "A client-side mod which prevent you from accidentally hurting other's pets.",
	"authors": [
		"celesteMoon"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/idwthp",
		"sources": "https://github.com/celesteMoon/idwthp"
	},
	"license": "GPL-3.0",
	"icon": "assets/idwthp/icon.png",
	"environment": "client",
	"entrypoints": {
		"main": [
			"celestemoon.idwthp.IDontWantToHurtPets"
		],
		"client": [
			"celestemoon.idwthp.IDontWantToHurtPetsClient"
		]
	},
	"mixins": [
		"idwthp.mixins.json",
		{
			"config": "idwthp.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}